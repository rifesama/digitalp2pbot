{
    "compilerOptions": {
        "outDir": "./digitalp2pbot-utils",
        "target": "es2020",
        "module": "commonjs",
        "preserveConstEnums": true,
        "noEmit": false,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": true
    },
    "include": ["src/**/*", "test/**/*"]
}
